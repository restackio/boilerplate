{
  "name": "@boilerplate/database",
  "version": "0.0.1",
  "private": true,
  "description": "Database schema and seed data for the boilerplate",
  "main": "index.js",
  "scripts": {
    "postgres:reset": "psql $DATABASE_URL -f postgres-schema.sql",
